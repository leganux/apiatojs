{
  "name": "apiato-typescript",
  "version": "3.1.1",
  "description": "A powerful and flexible API library for Node.js that supports both SQL (Sequelize) and NoSQL (Mongoose) databases, with REST and Socket.IO implementations",
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "bun build ./src/index.ts --outdir dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "api",
    "rest",
    "socket.io",
    "mongoose",
    "sequelize",
    "typescript",
    "orm",
    "database",
    "sql",
    "nosql",
    "mongodb",
    "sqlite",
    "mysql",
    "postgresql"
  ],
  "author": "leganux.net",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "moment": "^2.30.1",
    "mongoose": "^8.1.2",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.1",
    "socket.io": "^4.7.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.19",
    "@types/socket.io": "^3.0.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "express": "^4.x",
    "mongoose": "^8.x",
    "sequelize": "^6.x",
    "socket.io": "^4.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leganux/apiatojs.git"
  },
  "bugs": {
    "url": "https://github.com/leganux/apiatojs/issues"
  },
  "homepage": "https://github.com/leganux/apiatojs#readme"
}
